{
    "compilerOptions": {
        "preserveConstEnums": true,
        "strict": true,
        "target": "ES6",
        "module": "ESNext",
        "outDir": "./out",
        "rootDir": "./src",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    }
}